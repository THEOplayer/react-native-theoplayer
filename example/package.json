{
  "name": "react-native-theoplayer-example",
  "description": "Example app for react-native-theoplayer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "android-release": "npx react-native run-android --mode release",
    "ios": "npx react-native run-ios",
    "web": "webpack-dev-server --config ./web/webpack.config.js --mode development",
    "web-release": "webpack --config ./web/webpack.config.js --mode production",
    "start": "npx react-native start",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "pod-install": "cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install",
    "pod-update": "cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod update"
  },
  "dependencies": {
    "@react-native-community/slider": "^4.5.5",
    "@theoplayer/react-native-ui": "^0.9.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "npm:react-native-tvos@^0.76.5-0",
    "react-native-status-bar-height": "^2.6.0",
    "react-native-svg": "^15.8.0",
    "react-native-web": "^0.19.13",
    "react-native-web-image-loader": "^0.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.9",
    "@babel/preset-env": "^7.25.9",
    "@babel/runtime": "^7.25.9",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.76.5",
    "@react-native/eslint-config": "0.76.5",
    "@react-native/metro-config": "0.76.5",
    "@react-native/typescript-config": "0.76.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "babel-loader": "^8.4.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-react-native-web": "^0.17.7",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^8.57.1",
    "html-webpack-plugin": "^5.6.3",
    "react-native-svg-web": "^1.0.9",
    "typescript": "5.0.4",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "overrides": {
    "react-native-google-cast": {
      "react-native": "npm:react-native-tvos@^0.76.5-0"
    }
  },
  "engines": {
    "node": ">=18"
  }
}
